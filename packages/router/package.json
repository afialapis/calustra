{
  "name": "calustra-router",
  "version": "0.0.36",
  "description": "A router",
  "author": "Donato Lorenzo <donato@afialapis.com>",
  "contributors": [
    "Donato Lorenzo <donato@afialapis.com>"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/afialapis/calustra.git"
  },
  "bugs": {
    "url": "https://github.com/afialapis/calustra/issues"
  },
  "homepage": "https://calustra.afialapis.com/",
  "files": [
    "lib",
    "dist"
  ],
  "main": "lib/index.js",
  "cjs": "dist/calustra-router.cjs.js",
  "module": "dist/calustra-router.es.js",
  "scripts": {
    "lint": "eslint src",
    "test-clean": "rm -f test/bundle.js",
    "test-build": "rollup -c rollup.test.js",
    "test": "npm run test-clean && npm run test-build && ./node_modules/mocha/bin/mocha test/bundle.js",
    "lib-clean": "rm -rf lib && mkdir lib",
    "lib-build": "babel src -d lib",
    "lib": "npm run lib-clean && npm run lib-build",
    "dist-clean": "rm -rf dist && mkdir dist",
    "dist-build": "rollup -c",
    "dist": "npm run dist-clean && npm run dist-build",
    "clean": "npm run test-clean && npm run lib-clean && npm run dist-clean",
    "prepare": "npm run lint && npm run test && npm run lib && npm run dist"
  },
  "keywords": [
    "js",
    "orm",
    "sql"
  ],
  "dependencies": {
    "calustra": "workspace:../conn",
    "calustra-orm": "workspace:../orm",
    "koa-router": "10.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.2",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^3.1.0",
    "eslint": "^7.32.0",
    "koa": "^2.13.4",
    "mocha": "^9.2.0",
    "node-fetch": "^2.6.7",
    "rollup": "^2.67.2",
    "rollup-plugin-node-externals": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2"
  }
}
